extends ../admin

block prepend head
    title=title

block content
    .content-header
        .container-fluid
            .row.mb-2
                .col-sm-6
                    h1.m-0.text-dark Dashboard
                .col-sm-6
                    ol.breadcrumb.float-sm-right
                        li.breadcrumb-item 
                            a(href="/admin/") Home
                        li.breadcrumb-item.active Dashboard

    section.content
        .container-fluid
            .row
                //- .col-12.col-sm-6.col-md-3
                //-     .info-box
                //-         span.info-box-icon.bg-primary.elevation-1
                //-             i.fas.fa-industry
                //-         .info-box-content
                //-             span.info-box-text Request Count
                //-             span.info-box-number 18,9/s
                .col-12.col-sm-6.col-md-3
                    .info-box
                        span.info-box-icon.bg-danger.elevation-1
                            i.fas.fa-bolt
                        .info-box-content
                            span.info-box-text CPU Traffic
                            span.info-box-number#cpu 24,3%
                //- .col-12.col-sm-6.col-md-3
                //-     .info-box
                //-         span.info-box-icon.bg-warning.elevation-1
