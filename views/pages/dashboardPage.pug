                    .info-box
                        span.info-box-icon.bg-success.elevation-1
                            i.fas.fa-cogs
                        .info-box-content
                            span.info-box-text Engine Status
                            span.info-box-number Online
                                        
            .row
                .col-12
                    .card
                        .card-header
